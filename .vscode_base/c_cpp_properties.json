{
  "env": {
    "myDefaultIncludePath": [
      "${workspaceFolder}",
      "${workspaceFolder}/include"
    ],
    "myCompilerPath": "!installpath!mingw64/local/bin/g++"
  },
  "configurations": [
    {
      "name": "include paths",
      "intelliSenseMode": "gcc-x86",
      "includePath": [
        "!installpath!mingw64/include/gtk-4.0",
        "!installpath!mingw64/include/cairo",
        "!installpath!mingw64/include/pango-1.0",
        "!installpath!mingw64/include/harfbuzz",
        "!installpath!mingw64/include/pango-1.0",
        "!installpath!mingw64/include/fribidi",
        "!installpath!mingw64/include/cairo",
        "!installpath!mingw64/include/pixman-1",
        "!installpath!mingw64/include/freetype2",
        "!installpath!mingw64/include/libpng16",
        "!installpath!mingw64/include/gdk-pixbuf-2.0",
        "!installpath!mingw64/include/glib-2.0",
        "!installpath!mingw64/lib/glib-2.0/include",
        "!installpath!mingw64/include/graphene-1.0",
        "!installpath!mingw64/lib/graphene-1.0/include"
      ],
      "compilerPath": "!installpath!mingw64/bin/gcc.exe",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "browse": {
        "path": ["${workspaceFolder}"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
