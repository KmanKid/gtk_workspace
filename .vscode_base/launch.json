      "MIMode": "gdb",
      "miDebuggerPath": "!installpath!/mingw64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build GTK Application",
      "miDebuggerArgs": "",
      "logging": {
        "moduleLoad": false,
        "programOutput": true,
        "trace": false,
        "engineLogging": false,
        "traceResponse": false
      }
    }
  ]
